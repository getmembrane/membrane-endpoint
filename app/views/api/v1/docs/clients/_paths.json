"/clients/{id}": {
  "get": {
    "description":"<%= basic_permission_table('| NO | NO | NO | YES |').html_safe %> <%= description('Returns a user information') %>",
    "summary": "Find client by ID",
    "tags": [
      "clients"
    ],
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "description": "id of Client",
        "required": true,
        "type": "integer"
      }
    ],
    "responses": {
      "200": {
        "schema": {
          "$ref": "#/definitions/Client/wrapped"
        }
      },
      "401": {
        "description": "Unauthorized",
        "schema": {
          "$ref": "#/definitions/UnauthorizedError"
        }
      },
      "404": {
        "description": "Not found",
        "schema": {
          "$ref": "#/definitions/NotFoundError"
        }
      },
      "500": {
        "description": "Internal server error",
        "schema": {
          "$ref": "#/definitions/InternalServerError"
        }
      }
    }
  },
  "put": {
    "description":"<%= basic_permission_table('| NO | NO | NO | YES |').html_safe %> <%= description('Update an existing user') %>",
    "summary": "Update client profile",
    "tags": [
      "clients"
    ],
    "parameters": [
      {
        "name": "id",
        "in": "path",
        "description": "Client ID",
        "required": true,
        "type": "integer"
      }
    ],
    "responses": {
      "200": {
        "schema": {
          "$ref": "#/definitions/Client/wrapped"
        }
      }
    }
  }
},
"/clients": {
  "post": {
    "description":"<%= basic_permission_table('| NO | NO | NO | YES |').html_safe %> <%= description('Creates User assigned to OrganizationAccount') %>",
    "summary": "Add a new client",
    "tags": [
      "clients"
    ],
    "parameters": [
      {
        "id": "client[id]",
        "in": "formData",
        "description": "id",
        "required": true,
        "type": "string"
      }
    ],
    "responses": {
      "201": {
        "description": "Client created",
        "schema": {
          "$ref": "#/definitions/Client/wrapped"
        }
      },
      "401": {
        "description": "Unauthorized",
        "schema": {
          "$ref": "#/definitions/UnauthorizedError"
        }
      },
      "422": {
        "description": "Validation errors",
        "schema": {
          "$ref": "#/definitions/UnprocessableEntityError"
        }
      },
      "500": {
        "description": "Internal server error",
        "schema": {
          "$ref": "#/definitions/InternalServerError"
        }
      }
    }
  }
}
